# Supabase Self-Hosting Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL
POSTGRES_PASSWORD=your-super-secret-password-change-me

# JWT Secret (must be at least 32 characters)
# Generate with: openssl rand -base64 32
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

# API Keys (generate using JWT secret)
# Generate anon key with role=anon, iss=supabase
# Generate service role key with role=service_role, iss=supabase
# Use: https://supabase.com/docs/guides/self-hosting/docker#generate-api-keys
ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6IjxZT1VSX1BST0pFQ1RfSUQ+Iiwicm9sZSI6ImFub24iLCJpYXQiOjE2NDUxOTI4MjEsImV4cCI6MTk2MDc2ODgyMX0.abc123
SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6IjxZT1VSX1BST0pFQ1RfSUQ+Iiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImlhdCI6MTY0NTE5MjgyMSwiZXhwIjoxOTYwNzY4ODIxfQ.xyz789

# URLs
API_EXTERNAL_URL=http://localhost:8000
SITE_URL=http://localhost:3000

# Auth Settings
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
JWT_EXPIRY=3600

# SMTP Configuration (for email auth)
# Leave blank to use console logging for development
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_ADMIN_EMAIL=admin@example.com

# Additional redirect URLs (comma-separated)
ADDITIONAL_REDIRECT_URLS=
